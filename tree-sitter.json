{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "flix",
      "camelcase": "Flix",
      "title": "Flix",
      "scope": "source.flix",
      "file-types": [
        "flix"
      ],
      "injection-regex": "^flix$",
      "class-name": "TreeSitterFlix"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "Tree-sitter parser for the Flix programming language",
    "authors": [
      {
        "name": "Omar Jatoi",
        "email": "git@omarjatoi.com",
        "url": "https://omarjatoi.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/omarjatoi/tree-sitter-flix"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}